    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch Edge",
        "request": "launch",
        "type": "msedge",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}"
    },
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/omsproject/manage.py"
        }
    ]
}